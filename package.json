    "@types/write-file-atomic": "^4.0.3",
    "tsup": "^8.3.5",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  },
  "peerDependencies": {
    "pg": "^8.0.0",
    "mysql2": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "pg": { "optional": true },
    "mysql2": { "optional": true }
  },
  "engines": {
    "node": ">=18"
